#! /usr/bin/env python2.7
# -*- coding: utf-8 -*-

import sys
from lexer import *
from parser import *

data = open(sys.argv[1])
data = data.read()

lexer.input(data)

while True:    
    tok = lexer.token()
    if not tok: break

parser = yacc.yacc()

arbol = parser.parse(data)
print ">> " + str(arbol.getValue())
