#! /usr/bin/env python2.7
# -*- coding: utf-8 -*-

import sys
from lexer import *
from AST import *

data = open(sys.argv[1])
data = data.read()

lexer.input(data)

while True:    
    tok = lexer.token()
    if not tok: break

parser = yacc.yacc()

while True:
   try:
       s = raw_input('calc > ')
   except EOFError:
       break
   if not s: continue
   arbol = parser.parse(s)
   print "dio esto: " + str(arbol.getValue())